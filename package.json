{
  "name": "vue-stater",
  "description": "vue,leaflet,webpack,sass",
  "version": "1.0.0",
  "author": "locotor",
  "scripts": {
    "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js -w --progress --profile",
    "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --optimize-minimize  --progress --profile --bail",
    "build": "npm run build:prod",
    "clean:dist": "rimraf -- dist",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --progress --profile --watch --open --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "start": "npm run server:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch": "npm run watch:dev",
    "node": "node",
    "rimraf": "rimraf",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "core-js": "^2.5.1",
    "echarts": "^3.6.2",
    "element-ui": "^1.4.1",
    "esri-leaflet": "^2.1.0",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.2.0",
    "leaflet-editable": "^1.1.0",
    "leaflet-measure-path": "^1.3.1",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.1.0",
    "moment": "^2.20.1",
    "proj4": "^2.4.3",
    "proj4leaflet": "^1.0.1",
    "vue": "^2.4.2",
    "vue-cookie": "^1.1.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.2.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-plugin-html": "^3.1.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "optimize-js-plugin": "0.0.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
}
